{
  "extends": ["airbnb", "prettier", "react-app", "react-app/jest"],
  "plugins": ["prettier", "react-hooks"],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es6": true
  },
  "rules": {
    "react/jsx-filename-extension": "off",
    "react/jsx-props-no-spreading": "off",
    "react/prop-types": "off",
    "no-unused-expressions": "off"
  }
}
